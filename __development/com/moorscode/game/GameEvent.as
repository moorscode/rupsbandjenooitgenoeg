package com.moorscode.game {	import flash.events.Event;		public class GameEvent extends Event {		public static const LOADED:String = "gameLoaded";				public static const ENDED:String = "gameEnded";				public static const TICK:String = "gameTick";		public static const FRAME:String = "gameFrame";				public static const PAUSED:String = "gamePaused";		public static const UNPAUSED:String = "gameUnPaused";				public static const MUTED:String = "gameMuted";		public static const UNMUTED:String = "gameUnMuted";				public function GameEvent(type:String):void {			super(type);		}	}}
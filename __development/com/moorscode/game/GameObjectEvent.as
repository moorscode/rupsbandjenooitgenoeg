package com.moorscode.game {	import flash.events.Event;	import flash.geom.Point;		public class GameObjectEvent extends Event {		public static const DESTROYED:String = "Destroyed";		public static const CREATED:String = "Created";		public static const SHOOT:String = "Shoot";		public static const REMOVE:String = "Remove";				public var ownerId:uint = 0;		public var bulletId:uint = 0;		public var targetObject:IGameObject;		public var rotation:Number;		public var origin:Point;		public var speed:Number;				public var fforward:int = 0; // used for network data - remove the delay: client1 -> server -> client2, by extra ticking				public function GameObjectEvent(eventType:String):void {			super(eventType);		}	}}
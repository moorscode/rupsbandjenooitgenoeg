package com.moorscode.net {	import flash.events.Event;	public class NetEvent extends Event {		public static const CONNECT:String = "connect";		public static const DISCONNECT:String = "disconnect";		public static const ERROR:String = "error";		public static const DATA:String = "data";		public static const PING:String = "ping";				public static var __xml:XML = new XML();				// Constructor		function NetEvent(type:String) {			super(type);		}						public function set xml(value:XML):void {			__xml = value;		}				public function get xml():XML {			return __xml;		}	}}
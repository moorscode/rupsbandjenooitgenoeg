package com.moorscode.vlindervleugeltjevernielveel {	import flash.display.Sprite;	import flash.geom.Point;		import com.moorscode.game.IGameObject;		public class Tree extends IGameObject {		private var tree:Sprite;				public function Tree(position:Point):void {			super(position);			this.tree = Sprite(addChild(getSprite("tree")));						this.scaleX = this.scaleY = 0.7;			this.alpha = 0.8;		}				public override function unload():void {			// remove all attached objects.			while(numChildren > 0) {				removeChildAt(0);			}						tree = null;		}	}}
package com.moorscode.particles {	/*	 * The particle class, used together with the particleSystem class	 * @license Creative Commons by-nc-sa	 * @author Maxim Sprey <maximsprey@hotmail.com>	 * @version 0.5 - Still lots of room for improvement	 */	import flash.utils.getTimer;	import com.particles.particle;	public class bubbleParticle extends particle {				function bubbleParticle():void {			lifetime = 2000; // Particle lifetime			startSize = 5; // Particle size			endSize = 8;			startColor = 0xffffff; // Particle color			midColor = 0xffffff; // Particle end color			endColor = 0xffffff; // Particle end color			startBlur = 2; // Blur at start			endBlur = 6;			blurQuality = 3;			transparency = 1.0;			fadeAt = 0.9;		}				override public function drawShape():void {			graphics.clear();			graphics.lineStyle(1, 0x499DF5, trans, true);//			graphics.beginFill(color, 0.0);			graphics.drawCircle(this.pX, this.pY, size);//			graphics.endFill();		}	}}
package com.moorscode.particles {	/*	 * The particle class, used together with the particleSystem class	 * @license Creative Commons by-nc-sa	 * @author Maxim Sprey <maximsprey@hotmail.com>	 * @version 0.5 - Still lots of room for improvement	 */	import flash.utils.getTimer;	import com.particles.particle;	public class poisonParticle extends particle {				function poisonParticle():void {			lifetime = 1000; // Particle lifetime			size = 5; // Particle size			startColor = 0x698B22; // Particle color			midColor = 0xEEEE00; // Particle end color			endColor = 0xffffff; // Particle end color			startBlur = 2; // Blur at start			endBlur = 5;			blurQuality = 3;			transparency = 1.0;			fadeAt = 0.9;		}	}}
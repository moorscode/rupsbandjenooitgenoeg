package com.moorscode.particles {	/*	 * The particle class, used together with the particleSystem class	 * @license Creative Commons by-nc-sa	 * @author Maxim Sprey <maximsprey@hotmail.com>	 * @version 0.5 - Still lots of room for improvement	 */	import flash.utils.getTimer;	import com.particles.particle;	public class fireParticle extends particle {				function fireParticle():void {			lifetime = 1000; // Particle lifetime			startSize = 5; // Particle size			endSize = 8;			startColor = 0xFFE303; // Particle color			midColor = 0xDD0000; // Particle end color			endColor = 0x5E2612; // Particle end color			midColorPercentage = 0.6;			startBlur = 1; // Blur at start			endBlur = 15;			blurQuality = 3;			transparency = 1.0;			fadeAt = 0.9;		}	}}
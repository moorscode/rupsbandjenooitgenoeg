package com.moorscode.particles {	/*	 * The particle class, used together with the particleSystem class	 * @license Creative Commons by-nc-sa	 * @author Maxim Sprey <maximsprey@hotmail.com>	 * @version 0.5 - Still lots of room for improvement	 */	import flash.utils.getTimer;	import com.particles.particle;	public class smokeParticle extends particle {				function smokeParticle():void {			lifetime = 500; // Particle lifetime			size = 5; // Particle size			startColor = 0x000000; // Particle color			midColor = 0x000000; // Particle end color			endColor = 0x000000; // Particle end color			startBlur = 1; // Blur at start			endBlur = 10;			blurQuality = 3;			transparency = 1.0;			fadeAt = 0.4;		}	}}
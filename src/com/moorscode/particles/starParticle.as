package com.moorscode.particles {	/*	 * The particle class, used together with the particleSystem class	 * @license Creative Commons by-nc-sa	 * @author Maxim Sprey <maximsprey@hotmail.com>	 * @version 0.5 - Still lots of room for improvement	 */	import flash.utils.getTimer;	import com.particles.particle;	public class starParticle extends particle {				function starParticle():void {			lifetime = 1000; // Particle lifetime			size = 2; // Particle size			startColor = 0xFFE303; // Particle color			midColor = 0xEEEE00; // Particle end color			endColor = 0xffffff; // Particle end color			startBlur = 1; // Blur at start			endBlur = 2;			blurQuality = 1;			transparency = 1.0;			fadeAt = 0.8;		}				override public function drawShape():void {			graphics.clear();			graphics.lineStyle(1, 0xffffff, 0.5, false);			graphics.beginFill(color, trans);			graphics.drawCircle(pX, pY, size);			graphics.endFill();		}	}}
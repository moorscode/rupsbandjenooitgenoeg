package com.moorscode.rupsbandjenooitgenoeg {		import flash.display.MovieClip;	import flash.events.Event;	import flash.geom.Point;		import com.moorscode.game.IGameObject;		public class Explosion extends IGameObject {				private var explosion:MovieClip;				public function Explosion(position:Point):void {			super(position);						explosion = MovieClip(addChild(getMovieClip("Explosion")));			explosion.addEventListener(Event.ENTER_FRAME, frame);			explosion.gotoAndPlay(1);		}				public override function unload():void {			super.unload();						if(explosion) {				explosion.removeEventListener(Event.ENTER_FRAME, frame);				explosion = null;								if(parent && parent.contains(this)) {					parent.removeChild(this);				}			}		}				private function frame(event:Event):void {			if(event.target.currentFrame == event.target.totalFrames) {				unload();			}		}	}}
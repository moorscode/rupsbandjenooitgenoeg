package com.moorscode.rupsbandjenooitgenoeg {	import flash.display.Sprite;	import flash.geom.Point;		import com.moorscode.game.IGameObject;		public class Tree extends IGameObject {		private var tree:Sprite;				public function Tree(position:Point, tree:Sprite):void {			super(position);			this.tree = Sprite(addChild(tree));			rotation = targetRotation = Math.random() * 360;		}				public override function unload():void {			super.unload();						tree = null;		}				public override function hitRadius():Number {			return 2; // only use the stem of the tree!		}	}}
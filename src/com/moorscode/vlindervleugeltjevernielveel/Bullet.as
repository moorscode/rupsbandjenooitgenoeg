package com.moorscode.vlindervleugeltjevernielveel {	import flash.display.Sprite;	import flash.geom.Point;		import com.moorscode.game.IGameObject;		public class Bullet extends IGameObject {		private var bullet:Sprite;		public var owner:IGameObject;				public function Bullet(owner:IGameObject, position:Point, direction:Number, speed:Number):void {			super(position);						this.owner = owner;						this.bullet = Sprite(addChild(getSprite("bulletGfx")));						this.rotation = direction;//			this.direction = direction;//			this.rotation = this.direction;//			this.targetDirection = this.rotation;						this.speed = speed;			accelerating = true;						maxSpeed = 10;			accelerateSpeed = 0.8;		}				public override function unload():void {			while(numChildren > 0) {				removeChildAt(0);			}						this.owner = null			bullet = null;		}	}}